.container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  line-height: 2em;
  color: #161823;
  font: 14px/1.6 "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei",
    "WenQuanYi Micro Hei", "Helvetica Neue", Arial, sans-serif;
}

page {
  width: 100%;
  height: 100%;
  background-repeat: repeat-y;
  background-size: contain;
  background-image: url("data:image/jpg;base64,");
  -webkit-tap-highlight-color: #00000000;
  font: 14px/1.5 "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei",
    "WenQuanYi Micro Hei", "Helvetica Neue", Arial, sans-serif;
  text-align: justify;
}

.hl {
  color: #a61b29 !important;
  font-weight: 600;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}
